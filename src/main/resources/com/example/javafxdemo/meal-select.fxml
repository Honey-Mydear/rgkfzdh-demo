<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="mealAnchor"  layoutX="150.0" layoutY="97.0" prefHeight="400.0" prefWidth="600.0" style="-fx-border-color: black;"  xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.javafxdemo.controller.user.MealController"
>
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="200.0" layoutY="23.0" text="Select The Meal You Want" textOverrun="CENTER_ELLIPSIS">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="40.0" layoutY="60.0" text="The Staple Food">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="230.0" layoutY="60.0" text="The Halal Food">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="420.0" layoutY="60.0" text="Dessert And Drinks">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="mealconfirm" layoutX="200.0" layoutY="308.0" mnemonicParsing="false" onMouseClicked="#onClickConfirmButton" prefHeight="38.0" prefWidth="205.0" text="Next Page" />
        <Label fx:id="Staple" layoutX="40.0" layoutY="94.0" prefHeight="75.0" prefWidth="160.0" />
        <Label fx:id="Halal" layoutX="220.0" layoutY="94.0" prefHeight="75.0" prefWidth="160.0" />
        <Label fx:id="DessertDrink" layoutX="400.0" layoutY="94.0" prefHeight="75.0" prefWidth="160.0" />
        <Button fx:id="choosestaple" layoutX="60.0" layoutY="189.0" mnemonicParsing="false" text="Staple Food" onMouseClicked="#onClickChooseStaple"/>
        <Button fx:id="choosehalal" layoutX="260.0" layoutY="189.0" mnemonicParsing="false" text="Halal Food" onMouseClicked="#onClickChooseHalal"/>
        <Button fx:id="chooseDAD" layoutX="400.0" layoutY="189.0" mnemonicParsing="false" text="Dessert and Drinks (Additional payment)" onMouseClicked="#onClickChooseDAD"/>
    </children>
</AnchorPane>
